{
  "name": "@kurkov87/ngx-siema",
  "version": "7.0.0",
  "license": "MIT",
  "main": "lib/bundles/ngx-siema.umd.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "ngc": "ngc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:npm": "rimraf lib && ngc -p tsconfig.publish.es6.json && rollup -c rollup.publish.config.js && npm run postbuild:npm",
    "postbuild:npm": "cp src/ngx-siema/package.json LICENSE README.md CHANGELOG.md lib/",
    "test": "ng test",
    "lint": "ng lint ngx-siema --type-check",
    "e2e": "ng e2e",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "prepush": "npm run lint",
    "publish": "npm publish lib --access=public"

  },
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "core-js": "2.4.1",
    "rxjs": "^6.3.3",
    "siema": "1.4.6",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.9",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.79",
    "codelyzer": "3.0.1",
    "conventional-changelog-cli": "1.3.1",
    "husky": "0.14.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.2",
    "rimraf": "2.6.1",
    "rollup": "0.43.0",
    "ts-node": "3.0.6",
    "tslint": "^5.5.0",
    "tslint-language-service": "0.9.6",
    "typescript": "^3.1.6"
  }
